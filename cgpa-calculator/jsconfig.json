{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "checkJs": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
